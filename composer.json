{
    "name": "cheppers/minicrm-client",
    "description": "MiniCRM client",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "consolidation/robo": "^1.4",
        "phpunit/phpunit": "^8.2",
        "squizlabs/php_codesniffer": "^3.4",
        "sweetchuck/git-hooks": "^0.0.10",
        "sweetchuck/lint-report": "^0.0.10",
        "sweetchuck/robo-git": "^0.0.10",
        "sweetchuck/robo-phpcs": "^0.0.20",
        "webmozart/path-util": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Cheppers\\MiniCrm\\": "src/",
            "GuzzleHttp\\": "vendor/guzzlehttp/guzzle/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cheppers\\MiniCrm\\Tests\\": "tests/src"
        }
    },
    "scripts": {}
}
